{
  "name": "wyrihaximus/repository-template",
  "description": "Repository template generator and checker",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0 || ^5.6",
    "twig/twig": "^1.0",
    "symfony/yaml": "^2.7",
    "sebastian/diff": "^1.3",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "phake/phake": "^2.1",
    "squizlabs/php_codesniffer": "^2.8.1",
    "rskuipers/php-assumptions": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\RepositoryTemplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\RepositoryTemplate\\Tests\\": "tests/"
    }
  },
  "bin": [
    "repository-template-compare"
  ]
}
